    <script>
        $(document).ready(function () {
            $('.no-show').hide();
            $("#myInput").on("keyup", function () {
                var value = $(this).val().toLowerCase();
                $("#myTable tr").filter(function () {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
                if ($("#myTable tr:visible").length === 0) {
                    $('.no-show').show();
                    $('.headerTable').hide();
                } else {
                    $('.no-show').hide();
                    $('.headerTable').show();
                }
            });
        });
    </script>

